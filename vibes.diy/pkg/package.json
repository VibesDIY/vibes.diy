{
  "name": "vibes-diy",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "tsc": "core-cli tsc",
    "typecheck": "core-cli tsc --noEmit",
    "build": "echo",
    "dev": "deno run --allow-env --allow-run --allow-net --allow-read vibes-diy-srv.ts",
    "dev:netlify": "netlify dev",
    "start": "serve -s build/client",
    "test:router": "vitest run",
    "test": "cross-env DISABLE_REACT_ROUTER=true vitest run",
    "test:coverage": "cross-env DISABLE_REACT_ROUTER=true vitest run --coverage --watch",
    "test:watch": "vitest",
    "file-issues": "scripts/create-issues.sh",
    "test:file-issues": "scripts/test-issues.sh",
    "test:api-key": "node scripts/test-api-key.js",
    "test:credits": "node scripts/test-credits.js",
    "format": "prettier --check .",
    "check": "pnpm format:write && pnpm typecheck && pnpm test",
    "pack": "core-cli build --doPack",
    "publish": "core-cli build",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@adviser/cement": "^0.5.5",
    "@clerk/clerk-react": "^5.59.2",
    "@cloudflare/workers-types": "^4.20251221.0",
    "@fireproof/core": "0.24.3-dev-use-livequery",
    "@fireproof/core-runtime": "0.24.3-dev-use-livequery",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@shikijs/monaco": "^3.20.0",
    "@tailwindcss/typography": "^0.5.19",
    "@vibes.diy/hosting-base": "workspace:*",
    "@vibes.diy/prompts": "workspace:*",
    "@vibes.diy/use-vibes-base": "workspace:*",
    "@vibes.diy/use-vibes-types": "workspace:*",
    "call-ai": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.27.1",
    "isbot": "^5.1.25",
    "jose": "^6.1.3",
    "monaco-editor": "^0.55.1",
    "multiformats": "^13.4.2",
    "posthog-js": "^1.310.0",
    "react": "^19.2.3",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.11.0",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.4.0",
    "use-fireproof": "0.24.3-dev-use-livequery",
    "use-vibes": "workspace:*"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@cloudflare/vite-plugin": "^1.19.0",
    "@fireproof/core-cli": "0.24.3-dev-use-livequery",
    "@react-router/dev": "^7.11.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/react": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/babel__standalone": "^7.1.9",
    "@types/mime-types": "^3.0.1",
    "@types/react": "~19.2.7",
    "@types/react-dom": "~19.2.2",
    "@vitest/browser-playwright": "^4.0.16",
    "concurrently": "^9.1.2",
    "cross-env": "^10.0.0",
    "deno": "^2.5.6",
    "mime-types": "^3.0.2",
    "netlify": "^23.13.0",
    "netlify-cli": "^23.13.0",
    "prettier": "^3.7.4",
    "serve": "^14.2.5",
    "shiki": "^3.20.0",
    "storybook": "^10.1.10",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.15"
  }
}
