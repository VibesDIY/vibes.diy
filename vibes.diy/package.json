{
  "name": "ai-app-builder",
  "packageManager": "pnpm@10.7.1",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "netlify dev --offline",
    "dev:router": "react-router dev",
    "start": "serve -s build/client",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:router": "vitest run",
    "test": "DISABLE_REACT_ROUTER=true vitest run",
    "test:watch": "vitest",
    "test:chromium": "vitest run --config vitest.chromium.config.ts",
    "file-issues": "scripts/create-issues.sh",
    "test:file-issues": "scripts/test-issues.sh",
    "test:api-key": "node scripts/test-api-key.js",
    "test:credits": "node scripts/test-credits.js",
    "check": "pnpm format && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-router/node": "^7.4.1",
    "@react-router/serve": "^7.4.1",
    "@shikijs/monaco": "^3.2.1",
    "@tailwindcss/typography": "^0.5.16",
    "call-ai": "0.10.2",
    "isbot": "^5.1.25",
    "jose": "^6.0.10",
    "multiformats": "^13.3.7",
    "posthog-js": "^1.240.6",
    "react": "^19.1.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.1.0",
    "react-ga4": "^2.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-router": "^7.4.1",
    "use-fireproof": "0.20.5"
  },
  "devDependencies": {
    "@improvements/vite-plugin-devtools-json": "^0.0.3",
    "@react-router/dev": "^7.4.1",
    "@tailwindcss/vite": "^4.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.1.1",
    "concurrently": "^9.1.2",
    "jsdom": "^26.0.0",
    "netlify-cli": "^17.17.1",
    "playwright": "^1.54.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.8",
    "react-router-dom": "^7.5.0",
    "serve": "^14.2.4",
    "shiki": "^3.2.1",
    "tailwindcss": "^4.1.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  }
