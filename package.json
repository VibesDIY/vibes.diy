{
  "name": "ai-app-builder",
  "packageManager": "pnpm@10.13.1",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "netlify dev --offline",
    "dev:fplog": "FP_DEBUG='Loader,KeyBag' netlify dev --offline",
    "dev:router": "react-router dev",
    "start": "serve -s build/client",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:router": "vitest run",
    "test": "DISABLE_REACT_ROUTER=true vitest run",
    "test:watch": "vitest",
    "file-issues": "scripts/create-issues.sh",
    "test:file-issues": "scripts/test-issues.sh",
    "test:api-key": "node scripts/test-api-key.js",
    "test:credits": "node scripts/test-credits.js",
    "check": "pnpm format && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-router/node": "^7.7.1",
    "@react-router/serve": "^7.7.1",
    "@shikijs/monaco": "^3.8.1",
    "@tailwindcss/typography": "^0.5.16",
    "call-ai": "0.10.2",
    "isbot": "^5.1.29",
    "jose": "^6.0.12",
    "posthog-js": "^1.258.3",
    "react": "^19.1.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.1.1",
    "react-ga4": "^2.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-router": "^7.7.1",
    "use-fireproof": "0.20.5"
  },
  "devDependencies": {
    "@improvements/vite-plugin-devtools-json": "^0.0.3",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "jsdom": "^26.1.0",
    "netlify-cli": "^23.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react-router-devtools": "^5.0.6",
    "react-router-dom": "^7.7.1",
    "serve": "^14.2.4",
    "shiki": "^3.8.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
