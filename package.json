    "test:watch": "vitest",
    "file-issues": "scripts/create-issues.sh",
    "test:file-issues": "scripts/test-issues.sh",
    "check": "pnpm format:check && pnpm typecheck && pnpm test"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@codesandbox/sandpack-react": "latest",
    "@monaco-editor/react": "^4.7.0",
    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "@shikijs/monaco": "^3.2.1",
    "@tailwindcss/typography": "^0.5.16",
    "call-ai": "0.6.0",
    "isbot": "^5.1.17",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.1.5",
    "use-fireproof": "0.20.0-dev-preview-60"
  },
  "devDependencies": {
    "@react-router/dev": "^7.1.5",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.8",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.0",
    "shiki": "^3.2.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8"
  }
